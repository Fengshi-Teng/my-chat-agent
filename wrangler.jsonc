{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "my-chat-agent",
  "main": "src/server.ts",
  "compatibility_date": "2025-08-03",
  "compatibility_flags": ["nodejs_compat"],
  "ai": {
    "binding": "AI",
    "remote": true
  },
  "assets": {
    "directory": "public"
  },
  "durable_objects": {
    "bindings": [
      {
        "name": "Chat",
        "class_name": "Chat"
      }
    ]
  },
  "migrations": [
    {
      "tag": "v1",
      "new_sqlite_classes": ["Chat"]
    }
  ],
    "d1_databases": [
    {
      "binding": "DB",
      "database_name": "securityprice",
      "database_id": "34659e26-bae7-4ed1-8f88-3a210be361a6"
    }
  ]

}
